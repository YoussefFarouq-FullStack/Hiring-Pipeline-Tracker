{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/HiringPipelineUI/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_VERSION": "18"
  },
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/HiringPipelineUI/browser",
  "installCommand": "npm install",
  "framework": "angular"
}
